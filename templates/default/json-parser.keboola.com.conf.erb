<VirtualHost *:80>
  ServerName <%= @params[:server_name] %>
  ServerAlias <% @params[:server_aliases].each do |a| %><%= a %> <% end %>

    ErrorLog "|/usr/bin/logger -t 'apache_json-parser_err' -p local1.info"
    CustomLog "|/usr/bin/logger -t 'apache_json-parser' -p local1.info" combined

    php_admin_value error_log syslog
    php_flag display_startup_errors off
    php_admin_flag log_errors on
    php_flag display_errors off
    php_admin_value max_execution_time 18000
    php_admin_value memory_limit -1

    <Directory /www/jsonparser-api/web/>
	     Require all granted
        AllowOverride All
    </Directory>

</VirtualHost>
